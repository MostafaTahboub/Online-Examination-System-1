{
  "name": "online-examination-system",
  "version": "1.0.0",
  "description": "system for managing exams and grading for students ",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "tsc && jest",
    "build": "tsc --project .",
    "build-tsc": "tsc -w",
    "run-watch": "nodemon ./dist/index.js",
    "start-consumer": "node ./dist/routes/consumer.rs",
    "dev": "concurrently \"npm:build-tsc\"  \"npm:run-watch\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration": "typeorm migration:create ./migration/CreateAdmin",
    "migration-revert": "typeorm migration:revert -- -d ./dist/DB/dataSource.js"
  },
  "repository": {
    "type": "git",
    "url": "Online-Examination-System"
  },
  "author": "Mohammed Taradeh && mostafa tahboub",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.438.0",
    "aws-sdk": "^2.1482.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "sqs-consumer": "^7.4.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.4",
    "@types/sqs-consumer": "^5.0.0",
    "@types/supertest": "^2.0.15",
    "@types/uuid": "^9.0.6",
    "@types/validator": "^13.11.2",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
